						<script>
							function validateEmail797491000000399099 (){
								var form = document.forms[ 'WebToLeads797491000000399099' ];
								var emailFld = form.querySelectorAll ( '[ftype=email]' );
								var i;
								for( i = 0; i < emailFld.length; i++ ){
									var emailVal = emailFld[ i ].value;
									if((emailVal.replace( /^\s+|\s+$/g,'' )).length != 0 ){
										var atpos = emailVal.indexOf ( '@' );
										var dotpos = emailVal.lastIndexOf ( '.' );
										if( atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= emailVal.length ){
											alert ( 'Please enter a valid email address. ' );
											emailFld[ i ].focus ();
											return false;
										}
									}
								}
								return true;
							}
							function checkMandatory797491000000399099 (){
								var mndFileds = new Array ( 'Last Name' );
								var fldLangVal = new Array ( 'Last\x20Name' );
								for( i = 0; i < mndFileds.length; i++ ){
									var fieldObj = document.forms[ 'WebToLeads797491000000399099' ][ mndFileds[ i ]];
									if( fieldObj ){
										if(((fieldObj.value ).replace( /^\s+|\s+$/g,'' )).length == 0 ){
											if( fieldObj.type == 'file' ){
												alert ( 'Please select a file to upload.' );
												fieldObj.focus ();
												return false;
											}
											alert ( fldLangVal[ i ] + ' cannot be empty.' );
											fieldObj.focus ();
											return false;
										}
										else if( fieldObj.nodeName == 'SELECT' ){
											if( fieldObj.options[fieldObj.selectedIndex].value == '-None-' ){
												alert ( fldLangVal[ i ] + ' cannot be none.' );
												fieldObj.focus ();
												return false;
											}
										}
										else if( fieldObj.type == 'checkbox' ){
											if( fieldObj.checked == false ){
												alert ( 'Please accept  ' + fldLangVal[ i ]);
												fieldObj.focus ();
												return false;
											}
										}
										try {
											if( fieldObj.name == 'Last Name' ){
												name = fieldObj.value;
											}
										}
										catch ( e ){}
									}
								}
								trackVisitor797491000000399099 ();
								if( !validateEmail797491000000399099()){
									return false;
								}
								var urlparams = new URLSearchParams ( window.location.search );
								if( urlparams.has( 'service' ) && ( urlparams.get( 'service' ) === 'smarturl' )){
									var webform = document.getElementById ( 'webform797491000000399099' );
									var service = urlparams.get ( 'service' );
									var smarturlfield = document.createElement ( 'input' );
									smarturlfield.setAttribute ( 'type', 'hidden' );
									smarturlfield.setAttribute ( 'value', service );
									smarturlfield.setAttribute ( 'name', 'service' );
									webform.appendChild ( smarturlfield );
								}
								document.querySelector ( '.crmWebToEntityForm .formsubmit' ).setAttribute ( 'disabled', true );
							}
							function tooltipShow797491000000399099 ( el ){
								var tooltip = el.nextElementSibling;
								var tooltipDisplay = tooltip.style.display;
								if( tooltipDisplay == 'none' ){
									var allTooltip = document.getElementsByClassName ( 'zcwf_tooltip_over' );
									for( i = 0; i < allTooltip.length; i++ ){
										allTooltip[ i ].style.display = 'none';
									}
									tooltip.style.display = 'block';
								}
								else {
									tooltip.style.display = 'none';
								}
							}
						</script>
						<script type = 'text/javascript' id = 'VisitorTracking'>
							var $zoho = $zoho || {};
							$zoho.salesiq = $zoho.salesiq || {
								widgetcode:'siqf22bb123db21526fa95a85b6cd3826f45c01719153a25d098d5990fcce2acf7a',
								 values: {},
								ready: function (){}
							};
							var d = document;
							s = d.createElement ( 'script' );
							s.type = 'text/javascript';
							s.id = 'zsiqscript';
							s.defer = true;
							s.src = 'https://salesiq.zoho.in/widget';
							t = d.getElementsByTagName ( 'script' )[0];
							t.parentNode.insertBefore ( s, t );
							function trackVisitor797491000000399099 (){
								try {
									if( $zoho ){
										var LDTuvidObj = document.forms[ 'WebToLeads797491000000399099' ][ 'LDTuvid' ];
										if( LDTuvidObj ){
											LDTuvidObj.value = $zoho.salesiq.visitor.uniqueid ();
										}
										var firstnameObj = document.forms[ 'WebToLeads797491000000399099' ][ 'First Name' ];
										if( firstnameObj ){
											name = firstnameObj.value + ' ' + name;
										}
										$zoho.salesiq.visitor.name ( name );
										var emailObj = document.forms[ 'WebToLeads797491000000399099' ][ 'Email' ];
										if( emailObj ){
											email = emailObj.value;
											$zoho.salesiq.visitor.email ( email );
										}
									}
								}
								catch ( e ){}
							}
